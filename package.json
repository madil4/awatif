{
  "name": "awatif",
  "description": "The First Web-based Parametric Structural Engineering Platform",
  "keywords": [
    "awatif",
    "parametricdesign",
    "computationaldesign",
    "structuralengineering",
    "parametricmodeling"
  ],
  "author": "Mohamed Adil",
  "license": "MIT",
  "homepage": "https://awatif.co/",
  "repository": {
    "type": "git",
    "url": "https://github.com/madil4/awatif"
  },
  "scripts": {
    "dev:fem": "npm run dev -w awatif-fem",
    "cpp:fem": "npm run cpp -w awatif-fem",
    "dev:math": "npm run dev -w awatif-math",
    "dev:mesh": "npm run dev -w awatif-mesh",
    "dev:proprietary": "npm run dev -w awatif-proprietary",
    "dev:ui": "npm run dev -w awatif-ui",
    "dev:examples": "npm run dev -w examples",
    "build:examples": "npm run build -w examples",
    "dev:website": "npm run dev -w website",
    "build:website": "npm run build -w website",
    "dedupe": "npm dedupe && echo 'Optimize build size when using same package in multiple packages'",
    "ncu": "ncu && echo 'Update npm packages'"
  },
  "workspaces": [
    "awatif-fem",
    "awatif-math",
    "awatif-mesh",
    "awatif-proprietary",
    "awatif-ui",
    "examples",
    "website"
  ],
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "mathjs": "^13.2.0"
  }
}
